<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.vatri.inventory.controllers.RegisterController">
    <children>
        <Label layoutX="218.0" layoutY="94.0" prefHeight="30.0" prefWidth="156.0" text="Registration" AnchorPane.leftAnchor="218.0" AnchorPane.rightAnchor="227.0">
