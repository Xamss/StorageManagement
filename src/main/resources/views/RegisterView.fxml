        <Label layoutX="133.0" layoutY="226.0" text="Password:" />
        <PasswordField id="" fx:id="passwordField" layoutX="200.0" layoutY="221.0" prefWidth="200.0" />
        <Button id="btnLogin" layoutX="307.0" layoutY="273.0" mnemonicParsing="false" onAction="#btnRegisterPressed" prefHeight="29.0" prefWidth="94.0" text="Register">
